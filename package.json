{
  "name": "@NiklasRosenstein/yassg",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Yet another static site generator.",
  "main": "yassg.py",
  "bin": {
    "yassg": "./cli.py"
  },
  "python-dependencies": {
    "Jinja2": ">=2.9.6",
    "Markdown": ">=2.6.8",
    "click": ">=6.7",
    "toml": ">=0.9.2"
  },
  "extensions": [
    "!require-import-syntax"
  ],
  "dist": {
    "include_files": [
      "cli.py",
      "yassg.py",
      "theme/*",
      "shortcodes/*",
      "README.md"
    ]
  }
}